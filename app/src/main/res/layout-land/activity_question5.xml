<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundy"
    tools:context=".quiz.Question5">

    <TextView
        android:id="@+id/questionText1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="58dp"
        android:layout_marginBottom="42dp"
        android:text="What type of programming language is Python?"
        app:layout_constraintBottom_toTopOf="@+id/q5a3B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/next5B"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:text="Finish Quiz"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ansText5" />

    <Button
        android:id="@+id/q5a1B"
        android:layout_width="0dp"
        android:layout_height="63dp"
        android:layout_marginStart="42dp"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="127dp"
        android:layout_marginRight="127dp"
        android:layout_marginBottom="80dp"
        android:background="@drawable/roundedbutton"
        android:text="High-level"
        app:layout_constraintBottom_toBottomOf="@+id/ansText5"
        app:layout_constraintEnd_toStartOf="@+id/q5a4B"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/q5a2B" />

    <Button
        android:id="@+id/q5a2B"
        android:layout_width="0dp"
        android:layout_height="62dp"
        android:layout_marginStart="42dp"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="125dp"
        android:layout_marginEnd="125dp"
        android:layout_marginRight="125dp"
        android:background="@drawable/roundedbutton"
        android:text="Medium-level"
        app:layout_constraintEnd_toStartOf="@+id/q5a3B"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/q5a3B"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/roundedbutton"
        android:text="Low-level"
        app:layout_constraintBottom_toTopOf="@+id/q5a4B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/q5a2B"
        app:layout_constraintTop_toBottomOf="@+id/questionText1" />

    <Button
        android:id="@+id/q5a4B"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="17dp"
        android:background="@drawable/roundedbutton"
        android:text="None of the above"
        app:layout_constraintBottom_toTopOf="@+id/ansText5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/q5a1B"
        app:layout_constraintTop_toBottomOf="@+id/q5a3B" />

    <Button
        android:id="@+id/conf5B"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="321dp"
        android:layout_marginRight="321dp"
        android:text="Confirm"
        app:layout_constraintBaseline_toBaselineOf="@+id/ansText5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ansText5" />

    <TextView
        android:id="@+id/ansText5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        android:layout_marginBottom="6dp"
        android:maxLines="1"
        android:text="Correct Answer"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/next5B"
        app:layout_constraintEnd_toStartOf="@+id/conf5B"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/q5a4B" />
</androidx.constraintlayout.widget.ConstraintLayout>